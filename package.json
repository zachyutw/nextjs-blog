{
    "name": "nextjs-starter",
    "version": "1.0.0",
    "private": true,
    "author": "zachyutw",
    "keywords": [
        "nextjs",
        "docker",
        "styled-components",
        "material-ui",
        "storybook",
        "jest"
    ],
    "config": {
        "project": "zachyutw",
        "port": "8080"
    },
    "scripts": {
        "dev": "NODE_OPTIONS='--inspect' next dev",
        "build": "next build",
        "start": "next start -p $npm_package_config_port",
        "docker-build": "docker build -t $npm_package_config_project/$npm_package_name . && docker run --name $npm_package_name -p $npm_package_config_port:$npm_package_config_port -d $npm_package_config_project/$npm_package_name",
        "docker": "docker stop $npm_package_name && docker rm $npm_package_name && docker build -t $npm_package_config_project/$npm_package_name . && docker run --name $npm_package_name -p $npm_package_config_port:$npm_package_config_port -d $npm_package_config_project/$npm_package_name",
        "echo": "echo $npm_package_config_project/$npm_package_name",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook",
        "test": "jest",
        "test-dev": "jest --watchAll",
        "test-majestic": "majestic",
        "backstop": "backstop test",
        "backstop-approve": "backstop approve",
        "cypress:open": "cypress open"
    },
    "dependencies": {
        "@material-ui/core": "^4.11.0",
        "@material-ui/icons": "^4.9.1",
        "autoprefixer": "^9.8.6",
        "clsx": "^1.1.1",
        "next": "9.3.5",
        "puppeteer": "^5.2.1",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "sass": "^1.26.10",
        "styled-components": "^5.2.0"
    },
    "devDependencies": {
        "@babel/core": "^7.11.6",
        "@babel/preset-env": "^7.11.5",
        "@babel/preset-react": "^7.10.4",
        "@storybook/addon-actions": "^6.0.21",
        "@storybook/addon-controls": "^6.0.21",
        "@storybook/addon-docs": "^6.0.21",
        "@storybook/addon-essentials": "^6.0.21",
        "@storybook/addon-links": "^6.0.21",
        "@storybook/addon-viewport": "^6.0.21",
        "@storybook/client-api": "^6.0.21",
        "@storybook/react": "^6.0.21",
        "@testing-library/dom": "^7.24.1",
        "@testing-library/jest-dom": "^5.11.4",
        "@testing-library/react": "^11.0.2",
        "@testing-library/react-hooks": "^3.4.1",
        "babel-jest": "^26.3.0",
        "babel-loader": "^8.1.0",
        "babel-plugin-styled-components": "^1.11.1",
        "cypress": "^5.1.0",
        "eslint": "^7.8.1",
        "eslint-plugin-mdx": "^1.8.2",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^26.4.2",
        "jest-config": "^26.4.2",
        "jest-puppeteer": "^4.4.0",
        "pixelmatch": "^5.2.1",
        "pngjs": "^5.0.0",
        "react-is": "^16.13.1",
        "react-test-renderer": "^16.13.1"
    }
}
